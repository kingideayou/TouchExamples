<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.examples.customtouch"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="9"
        android:targetSdkVersion="15" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="Touch Examples"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.examples.customtouch.MainActivity"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.examples.customtouch.TwoDimensionScrollActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.examples.customtouch.TwoDimensionGestureScrollActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.examples.customtouch.MultitouchActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.examples.customtouch.MoveLoggerActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.examples.customtouch.TouchForwardActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.examples.customtouch.TouchDelegateActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.examples.customtouch.TouchListenerActivity"
            android:screenOrientation="portrait" />
        <activity android:name="com.examples.customtouch.TouchInterceptActivity" />
    </application>

</manifest>